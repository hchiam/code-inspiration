{
  "name": "code-inspiration",
  "version": "1.0.0",
  "private": true,
  "description": "Capture snippets of code inspiration while on the go.",
  "dependencies": {
    "create-react-app": "^3.2.0",
    "react": "^16.9.0",
    "react-dom": "^16.10.2",
    "react-draggable": "^4.2.0"
  },
  "devDependencies": {
    "react-scripts": "^3.3.1",
    "selenium-side-runner": "^3.16.0"
  },
  "scripts": {
    "start": "npm run my-build; react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "side-test": "selenium-side-runner -c \"goog:chromeOptions.args=[--headless,--nogpu] browserName=chrome\" test.side",
    "eject": "react-scripts eject",
    "my-build": "mkdir public/src || true; cp -r src/* public/src"
  },
  "license": "MIT",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
